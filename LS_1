#include<stdio.h>
int main()
{
int key,a[10],n=7,i,flag=0;
for(i=0;i<n;i++){
printf("Enter the array elements:");
scanf("%d",&a[i]);
}
printf("Enter the key value:");
scanf("%d",&key);
for(i=0;i<n;i++)         //Linear search
{
if(a[i]==key)
{
printf("element is found at location %d",i);
flag=1;
break;
}
}
if(flag==0)
printf("Element is not found");
return 0;
}
